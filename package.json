  "scripts": {
    "clearDist": "rm -rf dist/",
    "start-db": "docker run --name db -e POSTGRES_PASSWORD=secret -p 5432:5432 -d postgres || docker start db",
    "watch": "tsc -w",
    "start-server": "nodemon --delay 3 --watch ./dist --exec babel-node dist/app/server.js || exit 1",
    "dev": "yarn clearDist && tsc && concurrently \"yarn run watch\" \"(sleep 3 && yarn run start-server)\"",
    "build": "yarn clearDist && tsc && babel -q dist/ -d dist/",
    "start": "babel-node dist/app/server.js",
    "tslint": "tslint --project .",
    "migrations:run": "yarn build && typeorm migrations:run",
    "migrations:revert": "yarn build && typeorm migrations:revert",
    "migrations:generate": "yarn build && typeorm migrations:generate",
    "artisan": "yarn build && node ./dist/artisan/cli"
  },
  "engines": {
    "node": "8.x.x"
  },
